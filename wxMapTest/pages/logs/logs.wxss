.topThree{
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
  text-align: center;
  justify-content: space-between;
  width: 100%;
}
.topThree>view{
  width: 33%;
}
.topThree .imgCon{
  background: url(second.png) center no-repeat;
  background-size: 70%;
}
.topThree .imgFirst{
  background-image: url(first.png);
}
.topThree .imgThird{
  background-image: url(third.png);
}
.topThree .imgCon image{
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  margin-top: 105rpx;
}
.topThree .imgFirst image{
  width: 150rpx;
  height: 150rpx;
  margin-top: 80rpx;
}
/* 普通排序 */
.lastList{
  line-height: 100rpx;
  border-bottom: 1px solid #E5E5E5;
  display: flex; 
  vertical-align:middle; 
  margin: 0 40rpx;
  padding-bottom: 10rpx;
}
.lastList image{
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  vertical-align: middle; 
}
.lastList .infoLeft{
  flex: 0.3;
  text-align: center;
}
.lastList .infoCenter{
  flex: 2;
}
.lastList .infoRight{
  flex: 0.7;
  color: #FF6000;
}
.log-list {
  display: flex;
  flex-direction: column;
   padding: 40rpx; 
}
.log-item {
  margin: 10rpx;
}
